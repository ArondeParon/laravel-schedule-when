{
    "name": "arondeparon/laravel-schedule-when",
    "description": "A command that enabled you see what the scheduler will do at a given moment in time.",
    "license": "MIT",
    "authors": [
        {
            "name": "Aron Rotteveel",
            "email": "rotteveel.aron@gmail.com",
            "homepage": "https://github.com/arondeparon"
        }
    ],
    "homepage": "https://github.com/arondeparon/laravel-schedule-when",
    "keywords": ["Laravel", "ScheduleWhen", "Scheduler", "crontab"],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.8",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ArondeParon\\ScheduleWhen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArondeParon\\ScheduleWhen\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArondeParon\\ScheduleWhen\\ScheduleWhenServiceProvider"
            ]
        }
    }
}
